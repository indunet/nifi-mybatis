<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.indunet.nifi.mapper.VehicleMapper">

  <select id="listVehicle" resultType="org.indunet.nifi.entity.VehicleDO">
        select
            id,
            vehicle_model_id as vehicleModeId,
            vehicle_code as vehicleCode
        from
            vehicle
  </select>

    <insert id = "save">
        insert into test_table(name, value)
            values('vehicle', 1);
    </insert>
</mapper>